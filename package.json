{
  "name": "@compass/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install || exit 0",
    "start": "npm run dev",
    "dev": "nx run-many --target=serve --all --parallel",
    "dev:compass-admin": "nx serve compass-admin",
    "dev:webclient": "nx serve webclient",
    "build": "nx run-many --target=build --all",
    "build:compass-admin": "npm run format && npm run lint:compass-admin && tsc --noEmit -p apps/compass-admin/tsconfig.json && nx build compass-admin",
    "build:webclient": "npm run format && npm run lint:webclient && tsc --noEmit -p apps/webclient/tsconfig.json && nx build webclient",
    "build:shared-ui": "nx build shared-ui",
    "preview": "npm run build && npm run preview:compass-admin && npm run preview:webclient",
    "preview:compass-admin": "npm run build:compass-admin && nx preview compass-admin",
    "preview:webclient": "npm run build:webclient && nx preview webclient",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --watch --verbose",
    "test:compass-admin": "jest --testPathPatterns=apps/compass-admin --coverage --collectCoverageFrom='apps/compass-admin/src/**/*.{ts,tsx}' --collectCoverageFrom='!apps/compass-admin/src/**/*.d.ts' --collectCoverageFrom='!apps/compass-admin/src/**/main.tsx' --collectCoverageFrom='!apps/compass-admin/src/**/vite-env.d.ts'",
    "test:compass-admin:watch": "jest --testPathPatterns=apps/compass-admin --watch --coverage --collectCoverageFrom='apps/compass-admin/src/**/*.{ts,tsx}' --collectCoverageFrom='!apps/compass-admin/src/**/*.d.ts' --collectCoverageFrom='!apps/compass-admin/src/**/main.tsx' --collectCoverageFrom='!apps/compass-admin/src/**/vite-env.d.ts'",
    "test:webclient": "jest --testPathPatterns=apps/webclient --coverage --collectCoverageFrom='apps/webclient/src/**/*.{ts,tsx}' --collectCoverageFrom='!apps/webclient/src/**/*.d.ts' --collectCoverageFrom='!apps/webclient/src/**/main.tsx' --collectCoverageFrom='!apps/webclient/src/**/vite-env.d.ts'",
    "test:webclient:watch": "jest --testPathPatterns=apps/webclient --watch --coverage --collectCoverageFrom='apps/webclient/src/**/*.{ts,tsx}' --collectCoverageFrom='!apps/webclient/src/**/*.d.ts' --collectCoverageFrom='!apps/webclient/src/**/main.tsx' --collectCoverageFrom='!apps/webclient/src/**/vite-env.d.ts'",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "e2e": "playwright test",
    "e2e:compass-admin": "playwright test --project=compass-admin-*",
    "e2e:webclient": "playwright test --project=webclient-*",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "e2e:chrome": "playwright test --project=*-chromium",
    "e2e:firefox": "playwright test --project=*-firefox",
    "e2e:webkit": "playwright test --project=*-webkit",
    "e2e:mobile": "playwright test --project=*-mobile-*",
    "e2e:report": "playwright show-report test/playwright-report",
    "e2e:install": "playwright install",
    "lint": "nx run-many --target=lint --projects=compass-admin,webclient",
    "lint:fix": "nx run-many --target=lint --projects=compass-admin,webclient -- --fix",
    "lint:compass-admin": "nx lint compass-admin",
    "lint:webclient": "nx lint webclient",
    "lint:shared-ui": "nx lint shared-ui",
    "format": "prettier --write . --ignore-path .prettierignore",
    "format:check": "prettier --check . --ignore-path .prettierignore",
    "type-check": "tsc --noEmit -p apps/compass-admin/tsconfig.json && tsc --noEmit -p apps/webclient/tsconfig.json",
    "type-check:compass-admin": "tsc --noEmit -p apps/compass-admin/tsconfig.json",
    "type-check:webclient": "tsc --noEmit -p apps/webclient/tsconfig.json",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "analyze:compass-admin": "nx build compass-admin && npx vite-bundle-analyzer apps/compass-admin/dist",
    "analyze:webclient": "nx build webclient && npx vite-bundle-analyzer apps/webclient/dist",
    "deps": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "clean": "nx reset && rimraf dist coverage .nx apps/node_modules",
    "clean:install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "docker:build": "docker compose -f docker-compose.prod.yml build",
    "docker:up": "docker compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker compose -f docker-compose.prod.yml down",
    "docker:logs": "docker compose -f docker-compose.prod.yml logs -f",
    "docker:build:compass-admin": "docker build -f Dockerfile.compass-admin -t compass-admin:latest .",
    "docker:build:webclient": "docker build -f Dockerfile.webclient -t webclient:latest .",
    "docker:run:compass-admin": "docker run -d -p 8080:3000 --name compass-admin compass-admin:latest",
    "docker:run:webclient": "docker run -d -p 8081:3000 --name webclient webclient:latest",
    "docker:stop": "docker stop compass-admin webclient || true",
    "docker:remove": "docker rm compass-admin webclient || true",
    "ci": "npm run lint && npm run type-check && npm run test:ci && npm run build",
    "release": "npm run ci && npm run build"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/mapbox-gl": "^3.4.1",
    "@untitledui/file-icons": "^0.0.9",
    "@untitledui/icons": "^0.0.19",
    "clsx": "^2.1.0",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^2.6.2",
    "input-otp": "^1.4.2",
    "mapbox-gl": "^3.14.0",
    "qr-code-styling": "^1.9.2",
    "react": "19.0.0",
    "react-aria-components": "^1.12.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.4.1",
    "@nx/eslint": "21.4.1",
    "@nx/eslint-plugin": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/playwright": "21.4.1",
    "@nx/react": "21.4.1",
    "@nx/vite": "21.4.1",
    "@nx/web": "21.4.1",
    "@nx/workspace": "21.4.1",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.1.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jest-transform-stub": "^2.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.4.1",
    "only-allow": "^1.2.1",
    "postcss": "^8.5.6",
    "prettier": "^2.6.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-i18next-loader": "^3.1.2",
    "vitest": "^3.0.0"
  },
  "workspaces": [
    "apps/*",
    "shared-ui"
  ]
}
